{
  "name": "makklays/yii2-upload-remote-photos",

  "type": "yii2-extension",

  "description": "Upload photos from remote host and save it on system of file (site) for the Yii2 framework",
  "version": "1.0",
  "keywords": ["yii2", "remote upload", "photo", "image"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Kuziv",
      "email": "makklays@gmail.com",
      "role": "Developer"
    }
  ],

  "require": {
    "php": "^5.3.3 || ^7.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.0"
  },

  "extra": {
    "bootstrap": "makklays\\uploadRemotePhotos\\Bootstrap",
  },

  "autoload": {
    "psr-4": {
      "makklays\\uploadRemotePhotos\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "makklays\\Test\\": "test"
    }
  }
}